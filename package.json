    "hoist-non-react-statics": "^3.3.2",
    "js-cookie": "^3.0.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "qiankun": "^2.10.6",
    "react": "^17.0.2",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.4",
    "react-redux": "^8.0.4",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.0",
