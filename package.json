{
  "name": "react-cli",
  "version": "1.0.0",
  "author": "jiaozhiye",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development ENV_CONFIG=dev node --max-old-space-size=4096 build/dev.js",
    "dev": "cross-env NODE_ENV=development ENV_CONFIG=dev webpack serve --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node --max-old-space-size=32768 build/prod.js",
    "build-dev": "cross-env NODE_ENV=production ENV_CONFIG=dev node --max-old-space-size=32768 build/prod.js",
    "build-tst": "cross-env NODE_ENV=production ENV_CONFIG=tst node --max-old-space-size=32768 build/prod.js",
    "build-uat": "cross-env NODE_ENV=production ENV_CONFIG=uat node --max-old-space-size=32768 build/prod.js",
    "build-pre": "cross-env NODE_ENV=production ENV_CONFIG=pre node --max-old-space-size=32768 build/prod.js",
    "build-gray": "cross-env NODE_ENV=production ENV_CONFIG=gray node --max-old-space-size=32768 build/prod.js",
    "lint": "eslint ./src --ext .js,.ts,.jsx,.tsx",
    "lint-fix": "eslint --fix ./src --ext .js,.ts,.jsx,.tsx",
    "prettier": "prettier -c --write '**/*'"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,ts,jsx,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "add-dom-event-listener": "^1.1.0",
    "antd": "^4.16.6",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "hoist-non-react-statics": "^3.3.2",
    "js-cookie": "^2.2.1",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "screenfull": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.14.0",
    "@babel/plugin-proposal-decorators": "^7.14.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.14.0",
    "@babel/preset-typescript": "^7.14.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "html-webpack-plugin": "^5.3.0",
    "husky": "^4.3.8",
    "less": "^4.1.0",
    "less-loader": "^10.0.0",
    "lint-staged": "^10.5.2",
    "mini-css-extract-plugin": "^1.3.6",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "ora": "^3.4.0",
    "postcss-loader": "^6.1.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.2.0",
    "rimraf": "^2.7.0",
    "style-loader": "^3.0.0",
    "style-resources-loader": "^1.4.1",
